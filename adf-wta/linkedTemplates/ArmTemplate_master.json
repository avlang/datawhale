{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-wta"
		},
		"ls_ADLS_dlswta_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_ADLS_dlswta'"
		},
		"ls_ADLS_dlswta_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlswta.dfs.core.windows.net"
		},
		"ls_HTTP_tennis_wta_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/JeffSackmann/tennis_wta/master/"
		},
		"st_monthlyTrigger_properties_pl_data_HTTP_to_ADLS_matches_parameters_yearStart": {
			"type": "int",
			"defaultValue": 1968
		},
		"st_monthlyTrigger_properties_pl_data_HTTP_to_ADLS_matches_parameters_yearEnd": {
			"type": "int",
			"defaultValue": 2021
		},
		"st_monthlyTrigger_properties_pl_data_HTTP_to_ADLS_rankings_parameters_fileNames": {
			"type": "string",
			"defaultValue": "wta_rankings_00s.csv,wta_rankings_10s.csv,wta_rankings_80s.csv,wta_rankings_90s.csv,wta_rankings_current.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-wta_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ADLS_dlswta_accountKey": {
						"value": "[parameters('ls_ADLS_dlswta_accountKey')]"
					},
					"ls_ADLS_dlswta_properties_typeProperties_url": {
						"value": "[parameters('ls_ADLS_dlswta_properties_typeProperties_url')]"
					},
					"ls_HTTP_tennis_wta_properties_typeProperties_url": {
						"value": "[parameters('ls_HTTP_tennis_wta_properties_typeProperties_url')]"
					},
					"st_monthlyTrigger_properties_pl_data_HTTP_to_ADLS_matches_parameters_yearStart": {
						"value": "[parameters('st_monthlyTrigger_properties_pl_data_HTTP_to_ADLS_matches_parameters_yearStart')]"
					},
					"st_monthlyTrigger_properties_pl_data_HTTP_to_ADLS_matches_parameters_yearEnd": {
						"value": "[parameters('st_monthlyTrigger_properties_pl_data_HTTP_to_ADLS_matches_parameters_yearEnd')]"
					},
					"st_monthlyTrigger_properties_pl_data_HTTP_to_ADLS_rankings_parameters_fileNames": {
						"value": "[parameters('st_monthlyTrigger_properties_pl_data_HTTP_to_ADLS_rankings_parameters_fileNames')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}